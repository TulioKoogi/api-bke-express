generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id    Int    @id @default(autoincrement())
  pass  String @db.VarChar(255)
  email String @unique(map: "email_UNIQUE") @db.VarChar(55)
  name  String @db.VarChar(100)
}
